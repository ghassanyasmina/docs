{
  "$schema": "https://mintlify.com/docs.json",
  "styling": {
    "eyebrows": "breadcrumbs"
  },
  "theme": "linden",
  "name": "Yasmina embedded insurance",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "logo": {
    "dark": "/logo/Vector-1.png",
    "light": "/logo/Vector.png"
  },
  "fonts": {
    "family": "Inter, sans-serif"
  },
  "favicon": "/favicon.svg",
  "api": {
    "params": {
      "expanded": "all"
    },
    "examples": {
      "defaults": "required"
    },
    "playground": {
      "display": "interactive",
      "proxy": false
    }
  },
  "contextual": {
    "options": [
      "chatgpt"
    ]
  },
  "integrations": {
    "intercom": {
      "appId": "yu8wo042"
    }
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Guide",
        "icon": "book",
        "groups": [
          {
            "group": "Getting Started",
            "expanded": true,
            "pages": [
              "introduction",
              "environments",
              "authentication",
              "developer"
            ]
          },
          {
            "group": "Insurance Integration Guides",
            "expanded": true,
            "pages": [
              {
                "group": "Single Insurance Provider",
                "expanded": true,
                "pages": [
                  "single-provider-integrations/medical",
                  "single-provider-integrations/car",
                  "single-provider-integrations/cartpl",
                  "single-provider-integrations/property",
                  "single-provider-integrations/travel",
                  "single-provider-integrations/life",
                  "single-provider-integrations/horse"
                ]
              },
              {
                "group": "Multiple Insurance Providers",
                "expanded": true,
                "pages": [
                  "multi-provider-integrations/car",
                  "multi-provider-integrations/cartpl",
                  "multi-provider-integrations/property"
                ]
              }
            ]
          },
          {
            "group": "API Resources",
            "pages": [
              "api-resources/data",
              "api-resources/errors",
              "api-resources/rate"
            ]
          },
          {
            "group": "Webhooks",
            "pages":[
              "webhooks/guide"
            ]
          },
          {
            "group": "White-labeling Solutions",
            "pages": [
              "whitelabel/iframes",
              "whitelabel/snippets"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "icon": "file-code",
        "groups": [
          {
            "group": "Authentication",
            "openapi": {
              "source": "auth-api-reference/openapi.json",
              "directory": "auth-api-reference"
            }
          },
          {
            "group": "SME Medical APIs",
            "openapi": {
              "source": "medical-api-reference/openapi.json",
              "directory": "medical-api-reference"
            }
          },
          {
            "group": "Motor Insurance APIs (Multiple Providers)",
            "openapi": {
              "source": "car-api-reference/openapi.json",
              "directory": "car-api-reference"
            }
          },
          {
            "group": "TPL Motor Insurance APIs",
            "openapi": {
              "source": "cartpl-api-reference/openapi.json",
              "directory": "cartpl-api-reference"
            }
          },
          {
            "group": "Property Insurance APIs",
            "openapi": {
              "source": "property-api-reference/openapi.json",
              "directory": "property-api-reference"
            }
          },
          {
            "group": "Travel Insurance APIs",
            "openapi": {
              "source": "yasmina-schengen-travel-insurance/openapi.json",
              "directory": "yasmina-schengen-travel-insurance"
            }
          },
          {
            "group": "Life (S&P) Insurance APIs",
            "openapi": {
              "source": "life-snp/openapi.json",
              "directory": "life-snp"
            }
          },
          {
            "group": "Horse Insurance APIs",
            "openapi": {
              "source": "horse-api-reference/openapi.json",
              "directory": "horse-api-reference"
            }
          },
          {
            "group": "Payment",
            "openapi": {
              "source": "payments-api-reference/openapi.json",
              "directory": "payments-api-reference"
            }
          }
        ]
      },
      {
        "tab": "SDKs",
        "icon": "brackets-curly",
        "groups": [
          {
            "group": "Front-End SDKs",
            "pages": [
              "sdks/ios"
            ]
          }
        ]
      },
      {
        "tab": "Changelog",
        "icon": "clock-rotate-left",
        "groups": [
          {
            "group": "Changelog",
            "pages": [
              "changelog"
            ]
          }
        ]
      }
    ]
  },
  "background": {
    "color": {
      "light": "#FFF",
      "dark": "#000"
    }
  },
  "navbar": {
    "primary": {
      "type": "button",
      "label": "Portal",
      "href": "https://portal.yasmina.ai"
    }
  },
  "redirects": [
    {
      "source": "/integrations/property",
      "destination": "/multi-provider-integrations/property"
    },
    {
      "source": "/integrations/car",
      "destination": "/multi-provider-integrations/car"
    }
  ]
}